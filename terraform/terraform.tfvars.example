# Skopiuj ten plik do terraform.tfvars i wypełnij swoimi danymi

# Dane OVHCloud API (uzyskaj z: https://eu.api.ovh.com/createToken/)
ovh_application_key    = "your_application_key_here"
ovh_application_secret = "your_application_secret_here"
ovh_consumer_key      = "your_consumer_key_here"
ovh_service_name      = "your_project_id_here"

# Konfiguracja serwera Valheim
valheim_server_name = "Mój Serwer Valheim"
valheim_world_name  = "MojSwiat"
valheim_password    = "twoje_bezpieczne_haslo"

# Opcjonalne - dostosuj jeśli potrzebujesz
# instance_flavor = "s1-4"  # Większa instancja dla więcej graczy
# instance_region = "WAW1"  # Warszawa dla lepszego pingu